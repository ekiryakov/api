{% extends 'base.html.twig' %}

{% block title %}Vendor register{% endblock %}

{% block body %}
    <v-container fluid>
        <vendor-register csrf_token="{{ csrf_token('vendor_registration_form') }}"></vendor-register>
    </v-container>
{% endblock %}

{% block alert %}
    {% if errors|length > 0 %}
        <v-alert type="error">{{ errors[0].message }}</v-alert>
    {% endif %}
{% endblock %}