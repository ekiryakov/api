{% extends 'base.html.twig' %}

{% block title %}Customer register{% endblock %}

{% block body %}
    <v-container fluid>
        <customer-register _token="{{ json_encode(registrationForm._token) }}"></customer-register>
    </v-container>
{% endblock %}

{% block alert %}
    {% if error %}
        <v-alert type="error">{{ registrationForm.errors[0] }}</v-alert>
    {% endif %}
{% endblock %}