{% extends 'base.html.twig' %}

{% block title %}Customer register{% endblock %}

{% block body %}
    <v-container fluid>
        <customer-register _token="{{ csrf_token('register') }}"></customer-register>
    </v-container>
{% endblock %}

{% block alert %}
    {% if error %}
        <v-alert type="error">{{ error.messageKey|trans(error.messageData, 'security') }}</v-alert>
    {% endif %}
{% endblock %}