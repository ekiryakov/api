{% extends 'base.html.twig' %}

{% block title %}Customer register{% endblock %}

{% block body %}
    <v-container fluid>
        <customer-register _token="{{ form._token }}"></customer-register>
    </v-container>
{% endblock %}

{% block alert %}
    {% if error %}
        <v-alert type="error">{{ form.errors }}</v-alert>
    {% endif %}
{% endblock %}