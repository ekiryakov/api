{% extends 'base.html.twig' %}

{% block title %}Vendor login{% endblock %}

{% block body %}
    <v-container fluid>
        <vendor-login csrf_token="{{ csrf_token('authenticate') }}"></vendor-login>
    </v-container>
{% endblock %}

{% block alert %}
    {% if error %}
        <v-alert type="error">{{ error.messageKey|trans(error.messageData, 'security') }}</v-alert>
    {% endif %}
{% endblock %}
